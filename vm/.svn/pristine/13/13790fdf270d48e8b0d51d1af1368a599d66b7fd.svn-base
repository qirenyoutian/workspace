package com.py.bean;

import java.util.Date;

public class Banner {
    private Integer bannerId;

    private String bannerPic;

    private Integer bannerPointId;

    private Integer bannerEquipmentId;

    private Date bannerCreateTime;
    
    private Point point;
    
    private Equipment equipment;
    

    public Integer getBannerId() {
        return bannerId;
    }

    public void setBannerId(Integer bannerId) {
        this.bannerId = bannerId;
    }

    public String getBannerPic() {
        return bannerPic;
    }

    public void setBannerPic(String bannerPic) {
        this.bannerPic = bannerPic == null ? null : bannerPic.trim();
    }

    public Integer getBannerPointId() {
        return bannerPointId;
    }

    public void setBannerPointId(Integer bannerPointId) {
        this.bannerPointId = bannerPointId;
    }

    public Integer getBannerEquipmentId() {
        return bannerEquipmentId;
    }

    public void setBannerEquipmentId(Integer bannerEquipmentId) {
        this.bannerEquipmentId = bannerEquipmentId;
    }

    public Date getBannerCreateTime() {
        return bannerCreateTime;
    }

    public void setBannerCreateTime(Date bannerCreateTime) {
        this.bannerCreateTime = bannerCreateTime;
    }

	public Point getPoint() {
		return point;
	}

	public void setPoint(Point point) {
		this.point = point;
	}

	public Equipment getEquipment() {
		return equipment;
	}

	public void setEquipment(Equipment equipment) {
		this.equipment = equipment;
	}
}